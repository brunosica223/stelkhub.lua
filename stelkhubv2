-- ================ DETEC√á√ÉO DE MUNDO ================

local function detectLeafWorld()
    local leafKeywords = {
        "leaf", "village", "konoha", "naruto", "hokage",
        "folha", "kakashi", "sasuke", "sakura", "chakra",
        "leaf world", "leaf_village", "leaf-world"
    }
    
    for _, obj in ipairs(workspace:GetDescendants()) do
        local name = obj.Name:lower()
        for _, kw in ipairs(leafKeywords) do
            if name:find(kw) then
                inLeafWorld = true
                return "üçÉ LEAF WORLD", colors.success
            end
        end
    end
    
    inLeafWorld = false
    return "‚ùå FORA DO LEAF WORLD", colors.danger
end

-- ================ DETEC√á√ÉO DE GEMS E GOLD ================

local function getGemsAndGold()
    gems = 0
    gold = 0

    if player:FindFirstChild("leaderstats") then
        for _, child in ipairs(player.leaderstats:GetChildren()) do
            local name = child.Name:lower()
            if name:find("gem") then gems = child.Value or 0 end
            if name:find("gold") or name:find("coin") then gold = child.Value or 0 end
        end
    end

    return gems, gold
end

-- ================ SISTEMA DE STARS ================

local function findStars()
    local found = {}
    
    if not inLeafWorld then return found end
    
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj.Name == "Star" or obj.Name:sub(1,4) == "Star" or obj.Name:lower():find("star") then
            if obj:IsA("BasePart") or obj:IsA("Model") then
                table.insert(found, obj)
            end
        end
        
        if obj:IsA("ClickDetector") and obj.Parent then
            local parentName = obj.Parent.Name:lower()
            if parentName:find("star") then
                table.insert(found, obj.Parent)
            end
        end
        
        if #found >= 6 then break end
    end
    
    return found
end

local function spinStar(star)
    if not star then return false end

    local detector = star:FindFirstChildOfClass("ClickDetector")
    if detector then
        fireclickdetector(detector)
        return true
    end

    local prompt = star:FindFirstChildOfClass("ProximityPrompt")
    if prompt then
        prompt:InputHoldBegin()
        task.wait(0.1)
        prompt:InputHoldEnd()
        return true
    end

    return false
end

-- ================ SISTEMA DE MOBS ================

local function findMobs()
    local found = {}
    
    for _, obj in ipairs(workspace:GetDescendants()) do
        if obj:IsA("Model") and obj:FindFirstChildOfClass("Humanoid") then
            local name = obj.Name:lower()
            if name:find("mob") or name:find("boss") or name:find("enemy") or name:find("leaf") then
                table.insert(found, obj)
            end
        end
    end
    
    return found
end

-- ================ SISTEMA DE UNITS (PETS) - MELHORADO ================

local function findUnits()
    local found = {}
    
    -- Pastas comuns onde units podem estar
    local possiblePaths = {
        player:FindFirstChild("Units"),
        player:FindFirstChild("Pets"),
        player:FindFirstChild("Characters"),
        player:FindFirstChild("Inventory"),
        player:FindFirstChild("Bag"),
        player:FindFirstChild("Storage"),
        player:FindFirstChild("MyUnits"),
        player:FindFirstChild("UnitFolder"),
        player:FindFirstChild("DeployedUnits"),
        player:FindFirstChild("ActiveUnits")
    }
    
    for _, folder in ipairs(possiblePaths) do
        if folder then
            for _, child in ipairs(folder:GetChildren()) do
                if child:IsA("Folder") or child:IsA("Model") or child:IsA("Configuration") then
                    -- Tenta encontrar dano
                    local damage = 0
                    local damageValue = child:FindFirstChild("Damage") or 
                                        child:FindFirstChild("Power") or 
                                        child:FindFirstChild("Atk") or
                                        child:FindFirstChild("Attack")
                    if damageValue and damageValue:IsA("NumberValue") then
                        damage = damageValue.Value
                    end
                    
                    table.insert(found, {
                        Name = child.Name,
                        Damage = damage,
                        Object = child
                    })
                end
            end
        end
    end
    
    -- Se n√£o encontrou nada, procura em qualquer lugar dentro do jogador
    if #found == 0 then
        for _, child in ipairs(player:GetChildren()) do
            if child:IsA("Folder") or child:IsA("Model") then
                local damage = 0
                local damageValue = child:FindFirstChild("Damage") or child:FindFirstChild("Power")
                if damageValue and damageValue:IsA("NumberValue") then
                    damage = damageValue.Value
                end
                table.insert(found, {
                    Name = child.Name,
                    Damage = damage,
                    Object = child
                })
            end
        end
    end
    
    -- Ordenar por dano (maior primeiro)
    table.sort(found, function(a, b) return a.Damage > b.Damage end)
    
    return found
end

-- ================ EQUIPAR MELHOR UNIT ================

local function equipBestUnit()
    units = findUnits()
    
    if #units == 0 then
        print("‚ùå Nenhuma unit encontrada")
        return
    end
    
    local best = units[1]
    print("‚úÖ Melhor unit: " .. best.Name .. " (Dano: " .. best.Damage .. ")")
    
    if best.Object then
        local remote = player:FindFirstChild("EquipUnit") or 
                      player:FindFirstChild("SelectUnit") or
                      game:GetService("ReplicatedStorage"):FindFirstChild("EquipUnit")
        
        if remote and remote:IsA("RemoteEvent") then
            remote:FireServer(best.Object)
        end
        
        if player.Character then
            best.Object.Parent = player.Character
        end
    end
end

-- ================ AUTO KILL COM TELEPORTE ================

local function teleportToMob(mob)
    if not mob then return end
    local root = mob:FindFirstChild("HumanoidRootPart") or mob:FindFirstChild("Torso") or mob:FindFirstChild("UpperTorso")
    if root and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = root.CFrame + Vector3.new(0, 5, 0)
    end
end

local function attackMob(mob)
    if not mob then return end
    local humanoid = mob:FindFirstChildOfClass("Humanoid")
    if humanoid and humanoid.Health > 0 then
        -- Tenta danificar diretamente
        humanoid.Health = humanoid.Health - 1000 -- Dano simb√≥lico (ajuste)
        -- Se quiser que as units ataquem, talvez seja necess√°rio usar remotas
        -- Vamos deixar o dano direto como fallback
    end
end

local function startAutoKill()
    if not selectedMob then
        print("‚ùå Selecione um mob primeiro")
        return
    end
    
    autoKillActive = true
    print("‚öîÔ∏è Auto Kill ativado para: " .. selectedMob.Name)
    
    task.spawn(function()
        while autoKillActive and selectedMob and selectedMob.Parent do
            teleportToMob(selectedMob)
            attackMob(selectedMob)
            task.wait(1)
        end
        autoKillActive = false
        print("‚èπÔ∏è Auto Kill parado")
    end)
end

local function stopAutoKill()
    autoKillActive = false
end

-- ================ BYPASS ================

local function toggleBypass()
    if bypassActive then
        bypassActive = false
        return
    end

    bypassActive = true

    pcall(function()
        local mt = getrawmetatable and getrawmetatable(game) or getmetatable(game)
        if mt then
            local oldNamecall = mt.__namecall
            setreadonly(mt, false)

            mt.__namecall = newcclosure(function(self, ...)
                local method = getnamecallmethod()
                if method == "FireServer" then
                    local remoteName = tostring(self):lower()
                    if remoteName:find("anticheat") then
                        return
                    end
                end
                return oldNamecall(self, ...)
            end)
            setreadonly(mt, true)
        end
    end)
end

